{
  "version": 2,
  "buildCommand": "npm run build",
  "trailingSlash": true,
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "geolocation=(), microphone=(), camera=()"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://www.google.com https://fonts.googleapis.com https://vercel.live; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; img-src 'self' data: https:; font-src 'self' https://fonts.gstatic.com; connect-src 'self' https:; media-src 'self' https:; object-src 'none'; frame-src 'self' https://www.youtube.com https://www.google.com; worker-src 'self' blob:"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        },
        {
          "key": "Content-Type",
          "value": "text/plain; charset=UTF-8"
        }
      ]
    },
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        },
        {
          "key": "Content-Type",
          "value": "application/xml; charset=UTF-8"
        }
      ]
    },
    {
      "source": "/site.webmanifest",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        },
        {
          "key": "Content-Type",
          "value": "application/manifest+json; charset=UTF-8"
        }
      ]
    },
    {
      "source": "/(.*).json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json; charset=UTF-8"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/index",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/main",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/products",
      "destination": "/#products",
      "permanent": true
    },
    {
      "source": "/gallery",
      "destination": "/#gallery",
      "permanent": true
    },
    {
      "source": "/reviews",
      "destination": "/#reviews",
      "permanent": true
    },
    {
      "source": "/contacts",
      "destination": "/#contacts",
      "permanent": true
    },
    {
      "source": "/contact",
      "destination": "/#contacts",
      "permanent": true
    },
    {
      "source": "/about",
      "destination": "/#achievements",
      "permanent": true
    },
    {
      "source": "/services",
      "destination": "/#products",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/perila-iz-nerzhaveyuschey-stali-bishkek",
      "destination": "/#products"
    },
    {
      "source": "/lestnicy-dlya-basseyna-bishkek",
      "destination": "/#products"
    },
    {
      "source": "/karnizy-dlya-vannoy-bishkek",
      "destination": "/#products"
    },
    {
      "source": "/poruchni-lovz-bishkek",
      "destination": "/#products"
    },
    {
      "source": "/flagshtoki-iz-nerzhaveyuschey-stali-bishkek",
      "destination": "/#products"
    },
    {
      "source": "/truby-iz-nerzhaveyuschey-stali-bishkek",
      "destination": "/#products"
    },
    {
      "source": "/furnitura-iz-nerzhaveyuschey-stali-bishkek",
      "destination": "/#products"
    },
    {
      "source": "/kapsuly-vremeni-iz-nerzhaveyuschey-stali-bishkek",
      "destination": "/#products"
    },
    {
      "source": "/argonnaya-svarka-nerzhaveyuschey-stali-bishkek",
      "destination": "/#products"
    },
    {
      "source": "/aisi-304-316-201-nerzhaveyuschaya-stal-bishkek",
      "destination": "/#products"
    }
  ],
  "functions": {
    "app/**/*.js": {
      "runtime": "@vercel/node@3.0.5"
    }
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "env": {
    "COMPANY_NAME": "MaxMetall",
    "COMPANY_PHONE": "+996555918355",
    "COMPANY_EMAIL": "info@maxmetall.kg",
    "COMPANY_ADDRESS": "ул. Чуйкова 168, Бишкек, Кыргызстан",
    "COMPANY_WEBSITE": "https://maxmetall.kg",
    "SEO_KEYWORDS": "нержавейка бишкек, изделия из нержавейки, перила из нержавейки, ограждения из нержавейки, лестницы бассейн, карнизы ванная, поручни ЛОВЗ, флагштоки, аргонная сварка, AISI 304, AISI 316, MaxMetall, металлообработка, производство нержавейки, кыргызстан"
  },
  "regions": ["iad1"],
  "github": {
    "silent": true
  },
  "installCommand": "npm install",
  "devCommand": "npm run dev"
}
